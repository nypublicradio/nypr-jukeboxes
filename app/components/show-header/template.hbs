<div class="static-main-header">
  <div class="col" id="wqxr-logo" data-test-element="header-logo">
    {{#link-to "listen"}}
      {{svg-jar "wqxr-log-frq" class="site-header-title-logo"}}
    {{/link-to}}
  </div>
  <div class="col" id="live-nyc-txt-box">
    <p class="live-nyc-txt" data-test-element="header-tagline"> Live from NYC</p>
  </div>
</div>

{{#sticky-element class="site-header" as |state|}}
  <div class="horizontal-nav-container {{if state.isSticky "is-sticky"}}">
    {{horizontal-nav
      navRoot="listen"
      defaultSlug="listen"
      channelSlug="listen"
      links=links
      activeTabIndex=activeTabIndex
      clickTracking=true
      dataCategory="Non-Jukebox Player"
}}
  </div>
{{/sticky-element}}

<div class="donate-button-link">
  <a href="https://pledge3.wqxr.org/donate/main/onestep/?ref=button-donate-header" target="_blank" rel="noopener" class="donate-button-opacity">
    <button class="donate-button gtm__click-tracking" data-category="Non-Jukebox Player" data-action="Donate">
      Donate
    </button>
  </a>
</div>
